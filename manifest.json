{
  "name": "SignIn",
  "description": "Designed to allow quick and easy sign in to Google spreadsheet based QR codes.",
  "version": "1.0.0",
  "manifest_version": 1,
  "app": {
	  "background": {
	      "scripts": ["js/chrome.js"]
    	}
  },
  "permissions": [
    "storage"
  ],
  "icons": { "16": "res/chrome/icons/icon-16.png", "128": "res/chrome/icons/icon-128.png" }
}
